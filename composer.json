{
    "name": "mxr576/ddqg",
    "description": "Drupal Dependency Quality Gate - aims to helps with running Drupal projects on secure and high quality Drupal dependencies",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "Drupal",
        "QA",
        "dev",
        "dependency analyses"
    ],
    "authors": [
        {
            "name": "Dezső BICZÓ",
            "email": "mxr576@gmail.com"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "composer-runtime-api": "^2.2",
        "caseyamcl/guzzle_retry_middleware": "^2.12",
        "composer/semver": "^3.3",
        "guzzlehttp/guzzle": "^7.9.3",
        "halaxa/json-machine": "^1.1",
        "kevinrob/guzzle-cache-middleware": "^6.0.0",
        "league/flysystem": "^3.29.1",
        "loophp/collection": "^7.6.1",
        "prewk/xml-string-streamer": "^1.2",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.30",
        "friendsofphp/php-cs-fixer": "^3.75.0",
        "monolog/monolog": "^3.3",
        "phpstan/phpstan": "^2.1.12",
        "phpstan/phpstan-deprecation-rules": "^2.0.1",
        "symfony/stopwatch": "^6.2"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "mxr576\\ddqg\\": "src/"
        }
    },
    "bin": [
        "bin/ddqg-generate-no-unsupported-versions"
    ],
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "composer-normalize": {
            "indent-size": 4,
            "indent-style": "space"
        }
    }
}
